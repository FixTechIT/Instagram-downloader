            font-size: 20px;
            line-height: 60px;
            color: #ff5e62;
        }

        .step p {
            font-size: 12px;
        }

        .step span {
            display: block;
            background: #ffc6c7;
            color: #ff5e62;
            font-size: 20px;
            font-weight: bold;
            border-radius: 50%;
            border-color: #ff5e62;
            border-style: solid;
